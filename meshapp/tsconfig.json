{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 20",
  "compilerOptions": {
    "lib": ["es2023", "DOM"],
    "module": "Node16",
    "target": "es2022",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node16",
    "baseUrl": "."
  },
  "include": ["**/*.ts", "**/*.tsx", "src/models/*.d.ts"],
  "exclude": ["node_modules"]
}
